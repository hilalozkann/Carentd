*{
	margin: 0;
	padding: 0;
}
.header{
	height: 100vh;
}
.navbar-style{
	box-shadow: 0 5px 10px #efefef;
	background: rgba(0,0,0,0.6);
	text-transform: uppercase;
}
.logo{
	height: 48px;
	padding:2px 10px;
}
.icon-bar{
	background: orange;
}
.navbar-style.animate{
	top:-100px;
}
.navbar-style.sticky{
	top: 0;
	z-index: 99;
}